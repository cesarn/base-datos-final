CREATE TABLE VENTA (ID_VENTA INTEGER, PRECIO INTEGER, FECHA DATE, CANT_VENTA INTEGER)
SELECT * FROM VENTA;

CONSTRAINT PK_ID_VENTA PRIMARY KEY (ID_VENTA),

CREATE SEQUENCE SECUENCIA_PEDIDOS
START WITH 1
INCREMENT BY 1
NOMAXVALUE;

CREATE OR REPLACE PROCEDURE GUARDAR_VENTA (my_ID_VENTA IN INTEGER,PRECIO IN INTEGER, FECHA in DATE,CANT_VENTA IN INTEGER) 
AS 
BEGIN


SELECT SECUENCIA_VENTA.NEXTVAL INTO 
MY_ID_VENTA FROM DUAL;
INSERT INTO VENTA VALUES (MY_ID_VENTA, MY_PRECIO, MY_FECHA, MY_CANT_VENTA);
END;
/

