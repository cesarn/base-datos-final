CREATE TABLE AUTO  (ID_AUTO INTEGER, MARCA VARCHAR2 (120), MODELO VARCHAR2 (120),CANTIDAD INTEGER) 
INSERT INTO ALMACEN VALUES (1,'APPLE','IPHONE 6',20); 

SELECT * FROM AUTO;

CONSTRAINT PK_ID_AUTO PRIMARY KEY (ID_AUTO));

CREATE SEQUENCE SECUENCIA_AUTO
START WITH 1
INCREMENT BY 1
NOMAXVALUE;

CREATE OR REPLACE PROCEDURE GUARDAR_AUTO (my_ID_AUTO out INTEGER,MARCA in  VARCHAR2 (120), MODELO in VARCHAR2 (120),CANTIDAD out INTEGER) 
AS 
BEGIN

SELECT SECUENCIA_AUTO.NEXTVAL INTO 
MY_ID_AUTO FROM DUAL;
INSERT INTO AUTO VALUES (MY_ID_AUTO, MY_MARCA, MY_MODELO, MY_CANTIDAD);
END;
/

